{
  "expo": {
    "name": "Calculadora de Gorjeta",
    "slug": "Calculadora de Gorjeta",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EEE"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tipcalculator"
    },
    "android": {
      "package": "com.tipcalculator",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#EEE"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
